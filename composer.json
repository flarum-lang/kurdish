{
	"name": "flarum-lang/kurdish",
	"type": "flarum-extension",
	"description": "Kurdish language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/kurdish",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"ku-locale",
		"kurdish-locale",
		"kurdish"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/kurdish/issues",
		"source": "https://github.com/flarum-lang/kurdish"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Kurdish",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ku",
			"title": "Kurdish"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
